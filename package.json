{
  "name": "secret-hitler",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.html",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "npm install && run-p dev:*",
    "deploy": "npm run build && firebase deploy",
    "build": "tsc && parcel build ./src/secret-hitler/index.html --no-cache --no-minify --public-url=./",
    "dev:secret-hitler": "NODE_ENV=secret-hitler parcel ./src/secret-hitler/index.html --no-autoinstall --port 3333 --log-level 0",
    "dev:avalon": "NODE_ENV=avalon parcel ./src/secret-hitler/index.html --no-autoinstall --port 4444 --log-level 0",
    "dev:lint": "tsc -w",
    "precommit": "precise-commits"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/styled-jsx": "^2.2.7",
    "firebase-cli": "^1.2.0",
    "firebase-tools": "^6.1.2",
    "husky": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.15.3",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@types/ramda": "^0.25.45",
    "babel-polyfill": "^6.26.0",
    "firebase": "^5.7.0",
    "ramda": "^0.26.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-firebase-hooks": "^0.4.0",
    "styled-jsx": "^3.1.2"
  }
